      "server controller_ppc64 10.1.0.121:6080 check inter 2000 rise 2 fall 5"
    ],
    "neutron_api_ppc64": [
      "bind *:9696",
      "balance source",
      "option tcpka",
      "option httpchk",
      "option tcplog",
      "server controller_ppc64 10.1.0.121:9696 check inter 2000 rise 2 fall 5"
