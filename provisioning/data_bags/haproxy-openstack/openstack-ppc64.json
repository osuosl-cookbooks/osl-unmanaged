      "bind *:8774",
      "balance source",
      "option tcpka",
      "option httpchk",
      "option tcplog",
      "server controller_ppc64 10.1.0.121:8774 check inter 2000 rise 2 fall 5"
    ],
    "nova_metadata_api_ppc64": [
      "bind *:8775",
      "balance source",
      "option tcpka",
      "option tcplog",
      "server controller_ppc64 10.1.0.121:8775 check inter 2000 rise 2 fall 5"
