{
  "id": "ajats",
  "frontend": {
    "https_http": [
      "acl jenkins hdr_dom(host) ajats-jenkins.osuosl.org",
      "use_backend servers-ajats-ubuntu if jenkins"
    ],
    "http": [
      "http-request redirect scheme https code 301"
    ]
  },
  "backend": {
    "ajats-ubuntu": {
      "params": [
        "option httpchk"
      ],
      "servers": [
        {
          "hostname": "ajats-ubuntu",
          "ipaddress": "10.4.2.11",
          "port": "8080"
        }
      ]
    }
  },
  "ssl": {
    "resources": [
      {
        "name": "wildcard",
        "resource": "certificate_manage",
        "attributes": {
          "cert_file": "wildcard.pem",
          "key_file": "wildcard.key",
          "chain_file": "wildcard-bundle.crt",
          "combined_file": true
        }
      }
    ]
  }
}
