{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "image": {
        "image_upload": true,
        "upload_image": {
          "cirros": "http://download.cirros-cloud.net/daily/20150923/cirros-d150923-ppc64le-disk.img"
        },
        "upload_images": []
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "ceph": {
      "mon": {
        "role": "ceph_mon_ppc64",
        "tag": "ceph-mon-ppc64"
      },
      "osd": {
        "role": "ceph_osd_ppc64",
        "tag": "ceph-osd-ppc64"
      },
      "network": {
        "public": {
          "cidr": [
            "10.1.0.0/23"
          ]
        },
        "cluster": {
          "cidr": [
            "10.1.0.0/23"
          ]
        }
      }
    },
    "mysql": {
      "server_root_password": "rootpass",
      "server_debian_password": "debpass",
      "server_repl_password": "replpass",
      "allow_remote_root": true,
      "root_network_acl": [
        "%"
      ]
    },
    "osl-openstack": {
      "database_suffix": "x86",
      "databag_prefix": "x86",
      "ceph": true,
      "endpoint_hostname": "openpower-openstack.testing.osuosl.org",
      "db_hostname": "openpower-openstack.testing.osuosl.org",
      "image": {
        "glance_vol": "fs3.osuosl.bak:/openstack-glance"
      },
      "vxlan_interface": {
        "controller": {
          "default": "eth3"
        },
        "compute": {
          "default": "enP4p1s0.42"
        }
      },
      "physical_interface_mappings": [
        {
          "name": "public",
          "controller": {
            "default": "eth1"
          },
          "compute": {
            "default": "enP4p1s0.112"
          }
        }
      ]
    },
    "openstack": {
      "compute": {
        "libvirt": {
          "rbd": {
            "cinder": {
              "secret_uuid": "6f60c8cc-cd51-40e8-8860-913fb0b98124"
            }
          }
        }
      },
      "block-storage": {
        "conf": {
          "DEFAULT": {
            "volume_group": "openpower"
          }
        },
        "volume": {
          "create_volume_group_type": "block_devices"
        }
      },
      "dashboard": {
        "server_hostname": "openpower-openstack.testing.osuosl.org"
      }
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack ppc64 Provisioning Role",
  "name": "openstack_provisioning_ppc64"
}
