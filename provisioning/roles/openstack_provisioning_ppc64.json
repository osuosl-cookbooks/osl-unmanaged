{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "image": {
        "image_upload": true,
        "upload_image": {
          "cirros": "http://download.cirros-cloud.net/daily/20150923/cirros-d150923-ppc64le-disk.img"
        },
        "upload_images": []
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "firewall": {
      "iptables-config": {
        "save_on_restart": "yes",
        "save_on_stop": "yes"
      }
    },
    "mysql": {
      "server_root_password": "rootpass",
      "server_debian_password": "debpass",
      "server_repl_password": "replpass",
      "allow_remote_root": true,
      "root_network_acl": [
        "%"
      ]
    },
    "osl-openstack": {
      "database_suffix": "x86",
      "databag_prefix": "x86",
      "endpoint_hostname": "otest-openpower-controller.osuosl.org",
      "db_hostname": "otest-openpower-controller.osuosl.org",
      "image": {
        "glance_vol": "fs3.osuosl.bak:/openstack-glance"
      },
      "vxlan_interface": {
        "controller": "eth3",
        "compute": "enP3p5s0f3"
      },
      "physical_interface_mappings": [
        {
          "name": "public",
          "controller": {
            "default": "eth1"
          },
          "compute": {
            "default": "enP3p5s0f2"
          }
        },
        {
          "name": "vlan42",
          "controller": {
            "default": "eth2"
          },
          "compute": {
            "default": "enP3p5s0f3"
          }
        }
      ]
    },
    "openstack": {
      "block-storage": {
        "conf": {
          "DEFAULT": {
            "volume_group": "openpower"
          }
        },
        "volume": {
          "create_volume_group_type": "block_devices"
        }
      }
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack ppc64 Provisioning Role",
  "name": "openstack_provisioning_ppc64"
}
