{
  "name": "osl-openstack-ppc64",
  "description": "The OSL Openstack Environment (pp64)",
  "cookbook_versions": {
    "aliases": "= 0.1.1",
    "apache2": "= 1.10.0",
    "apt": "= 2.3.8",
    "ark": "= 0.8.0",
    "aws": "= 2.1.1",
    "base": "= 1.3.16",
    "bluepill": "= 2.3.1",
    "build-essential": "= 1.4.2",
    "certificate": "= 0.5.0",
    "chef-client": "= 3.3.8",
    "chef_handler": "= 1.1.5",
    "chef-server": "= 1.1.0",
    "cpu": "= 0.2.0",
    "cron": "= 1.2.2",
