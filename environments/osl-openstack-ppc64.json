{
    "chef_type": "environment",
    "cookbook_versions": {},
    "default_attributes": {},
    "description": "The OSL Openstack Environment (pp64)",
    "json_class": "Chef::Environment",
    "name": "osl-openstack-ppc64",
    "override_attributes": {
        "yum": {
          "epel": {
            "baseurl": "http://epel.osuosl.org/6/$basearch",
            "enable": "true",
            "gpgkey": "http://epel.osuosl.org/RPM-GPG-KEY-EPEL-6"
          }
        },
        "openstack": {
            "compute": {
                "config": {
                    "ram_allocation_ratio": 5.0
                },
                "libvirt": {
                    "virt_type": "kvm"
                },
                "network": {
                    "public_interface": "enP2p1s0f2",
                    "service_type": "neutron",
                    "multi_host": "true",
                    "floating": {
                        "public_network_name": "vlan108",
                        "ipv4_cidr": "140.211.168.0/24"
                    }
                },
                "networks": [
                    {
                        "bridge": "br-private",
                        "bridge_interface": "enP2p1s0f3",
                        "dns1": "140.211.166.130",
                        "dns2": "140.211.166.131",
                        "ipv4_cidr": "192.168.32.0/22",
                        "label": "private",
                        "multi_host": "T",
                        "network_size": "255",
                        "num_networks": "1"
                    },
                    {
                        "bridge": "br108",
                        "bridge_interface": "enP2p1s0f2",
                        "dns1": "140.211.166.130",
                        "dns2": "140.211.166.131",
                        "ipv4_cidr": "140.211.168.0/24",
                        "label": "public",
                        "multi_host": "T",
                        "network_size": "255",
                        "num_networks": "1"
                    }
                ]
            },
            "dashboard": {
                "keystone_default_role": "_member_"
            },
            "db": {
                "compute": {
                    "db_name": "nova_ppc64",
                    "username": "nova_ppc64"
                },
                "identity": {
                    "db_name": "keystone_ppc64",
                    "username": "keystone_ppc64"
                },
                "image": {
                    "db_name": "glance_ppc64",
                    "username": "glance_ppc64"
                },
                "block-storage": {
                    "db_name": "cinder_ppc64",
                    "username": "cinder_ppc64"
                },
                "dashboard": {
                    "db_name": "horizon_ppc64",
                    "username": "horizon_ppc64"
                },
                "telemetry": {
                    "db_name": "ceilometer_ppc64",
                    "username": "ceilometer_ppc64"
                },
                "orchestration": {
                    "db_name": "heat_ppc64",
                    "username": "heat_ppc64"
                },
                "network": {
                    "db_name": "neutron_ppc64",
                    "username": "neutron_ppc64"
                }
            },
            "developer_mode": false,
            "endpoints": {
                "bind-host": "0.0.0.0",
                "db": {
                    "host": "140.211.15.221"
                },
                "host": "140.211.167.193",
                "mq": {
                    "host": "140.211.167.193"
                }
            },
            "image": {
                "image_upload": true,
                "upload_image": {
                    "fedora-20": "http://ftp.osuosl.org/pub/osl/openpower/openstack/fedora-20.qcow2"
                },
                "upload_images": [
                    "fedora-20"
                ]
            },
            "secret": {
                "key_path": "/etc/chef/encrypted_data_bag_secret",
                "secrets_data_bag": "ppc64_secrets",
                "service_passwords_data_bag": "ppc64_service_passwords",
                "db_passwords_data_bag": "ppc64_db_passwords",
                "user_passwords_data_bag": "ppc64_user_passwords"
            },
            "yum": {
                "repo-key": "https://github.com/redhat-openstack/rdo-release/raw/master/RPM-GPG-KEY-RDO-Havana"
            }
        }
    }
}
