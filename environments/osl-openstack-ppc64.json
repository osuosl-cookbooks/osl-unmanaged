    "git": "= 4.0.2",
    "iptables": "= 0.13.0",
    "java": "= 1.22.0",
    "jenkins": "= 0.8.0",
    "line": "= 0.5.1",
    "logrotate": "= 1.5.0",
    "magic_shell": "= 0.2.0",
    "memcached": "= 1.7.2",
    "modules": "= 0.2.0",
    "monit": "= 0.7.1",
    "monitoring": "= 0.2.6",
    "munin": "= 1.4.3",
    "mysql": "= 5.5.1",
    "nagios": "= 4.4.2",
    "networking_basic": "= 0.0.5",
    "newrelic": "= 0.5.8",
    "nfs": "= 1.0.0",
    "ntp": "= 1.6.2",
    "ohai": "= 1.1.12",
    "omnibus_updater": "= 1.0.2",
    "openssh": "= 1.3.2",
    "openssl": "= 1.1.0",
    "openstack-block-storage": "= 9.4.0",
    "openstack-common": "= 9.7.0",
    "openstack-compute": "= 9.3.0",
    "openstack-dashboard": "= 9.1.0",
    "openstack-identity": "= 9.3.1",
    "openstack-image": "= 9.2.0",
    "openstack-network": "= 9.1.0",
    "openstack-object-storage": "= 9.0.3",
    "openstack-ops-database": "= 9.2.0",
    "openstack-ops-messaging": "= 9.0.1",
    "openstack-orchestration": "= 9.2.0",
    "openstack-telemetry": "= 9.2.0",
    "osl-apache": "= 1.0.12",
    "osl-munin": "= 0.1.2",
    "osl-mysql": "= 0.1.0",
    "osl-nodejs": "= 0.1.3",
    "osl-openstack": "= 1.0.8",
    "osl-postgresql": "= 0.1.17",
    "osl-sudo": "= 0.1.0",
    "perl": "= 1.2.2",
    "php": "= 1.2.4",
    "postfix": "= 3.6.0",
    "postgresql": "= 3.4.2",
    "python": "= 1.4.6",
    "rabbitmq": "= 3.1.0",
    "racktables": "= 1.3.3",
    "rdiff-backup": "= 1.0.4",
    "rsync": "= 0.8.4",
    "rsyslog": "= 1.12.2",
    "runit": "= 1.5.10",
    "selinux": "= 0.7.2",
    "simple_iptables": "= 0.6.0",
    "ssh-ldap": "= 0.1.0",
    "sudo": "= 2.5.2",
    "supervisor": "= 0.4.0",
    "sysctl": "= 0.6.0",
    "user": "= 0.3.1",
    "windows": "= 1.30.2",
    "xfs": "= 1.1.0",
    "xml": "= 1.2.4",
    "yum": "= 3.2.4",
    "yum-epel": "= 0.4.0",
    "zlib": "= 2.0.0"
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
  },
  "override_attributes": {
    "apache": {
      "contact": "hostmaster@osuosl.org"
    },
    "openstack": {
      "compute": {
        "config": {
          "allow_same_net_traffic": "False",
          "ram_allocation_ratio": 5.0
        },
        "libvirt": {
          "virt_type": "kvm"
        },
        "network": {
          "auto_assign_floating_ip": "true\ndefault_floating_pool=public",
          "force_dhcp_release": "true",
          "multi_host": "true",
          "public_interface": "enP2p1s0f2",
          "service_type": "nova"
        },
        "networks": [
          {
            "bridge": "br-tenant",
            "bridge_dev": "enP2p1s0f3",
            "dns1": "140.211.166.130",
            "dns2": "140.211.166.131",
            "ipv4_cidr": "192.168.60.0/22",
            "label": "private",
            "multi_host": "T",
            "network_size": "1024",
            "num_networks": "1"
          }
        ]
      },
      "dashboard": {
        "keystone_default_role": "_member_",
        "server_hostname": "openpower-openstack.osuosl.org",
        "ssl": {
          "cert": "horizon.pem",
          "cert_url": "file:///etc/pki/tls/certs/wildcard.pem",
          "chain": "wildcard-bundle.crt",
          "key": "horizon.key",
          "key_url": "file:///etc/pki/tls/private/wildcard.key"
        }
      },
      "db": {
        "block-storage": {
          "db_name": "cinder_ppc64",
          "username": "cinder_ppc64"
        },
        "compute": {
          "db_name": "nova_ppc64",
          "username": "nova_ppc64"
        },
        "dashboard": {
          "db_name": "horizon_ppc64",
          "username": "horizon_ppc64"
        },
        "identity": {
          "db_name": "keystone_ppc64",
          "username": "keystone_ppc64"
        },
        "image": {
          "db_name": "glance_ppc64",
          "username": "glance_ppc64"
        },
        "network": {
          "db_name": "neutron_ppc64",
          "username": "neutron_ppc64"
        },
        "orchestration": {
          "db_name": "heat_ppc64",
          "username": "heat_ppc64"
        },
        "telemetry": {
          "db_name": "ceilometer_ppc64",
          "username": "ceilometer_ppc64"
        }
      },
      "developer_mode": false,
      "endpoints": {
        "bind-host": "0.0.0.0",
        "db": {
          "host": "140.211.15.221"
        },
        "host": "140.211.167.193",
        "mq": {
          "host": "140.211.167.193"
        },
        "compute-xvpvnc": {
          "uri": "http://openpower-openstack.osuosl.org:6081/console"
        },
        "compute-novnc": {
          "uri": "https://openpower-openstack.osuosl.org:6080/vnc_auto.html"
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {
          "debian-7": "http://ftp.osuosl.org/pub/osl/openpower/openstack/debian-7.qcow2",
          "fedora-20": "http://ftp.osuosl.org/pub/osl/openpower/openstack/fedora-20.qcow2",
          "ubuntu-14.04": "http://ftp.osuosl.org/pub/osl/openpower/openstack/ubuntu-14.04.qcow2",
          "ubuntu-14.04-le": "http://ftp.osuosl.org/pub/osl/openpower/openstack/ubuntu-14.04-le.qcow2"
        },
        "upload_images": [
          "debian-7",
          "fedora-20",
          "ubuntu-14.04",
          "ubuntu-14.04-le"
        ]
      },
      "release": "icehouse",
      "secret": {
        "db_passwords_data_bag": "ppc64_db_passwords",
        "key_path": "/etc/chef/encrypted_data_bag_secret",
        "secrets_data_bag": "ppc64_secrets",
        "service_passwords_data_bag": "ppc64_service_passwords",
        "user_passwords_data_bag": "ppc64_user_passwords"
      },
      "sysctl": {
        "net.ipv4.conf.all.rp_filter": "0",
        "net.ipv4.conf.default.rp_filter": "0",
        "net.ipv4.ip_forward": "1"
      }
    },
    "yum": {
      "epel": {
        "baseurl": "http://epel.osuosl.org/6/$basearch",
        "enable": "true",
        "gpgkey": "http://epel.osuosl.org/RPM-GPG-KEY-EPEL-6"
      }
    }
  }
}
