{
  "name": "phpbb",
  "description": "phpbb environment",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "cookbook_versions": {
    "apache2": "= 3.1.0",
    "git": "= 4.0.2",
    "jira": "1.0.2",
    "mysql": "= 6.1.2",
    "ntp": "= 1.6.2",
    "osl-apache": "= 2.3.7",
    "osl-haproxy": "= 2.1.0",
    "osl-nagios": "= 2.1.2",
    "nagios": "= 7.2.4",
    "proj-phpbb": "= 2.2.1",
    "rdiff-backup": "= 2.0.6"
  },
  "default_attributes": {
    "nagios": {
      "allowed_hosts": [
        "140.211.167.157",
        "10.1.0.100",
        "140.211.166.151",
        "10.1.0.66"
      ]
    },
    "nrpe": {
      "allowed_hosts": [
        "140.211.167.157",
        "10.1.0.100",
        "140.211.166.151",
        "10.1.0.66"
      ]
    },
    "chef_client": {
      "init_style": "none",
