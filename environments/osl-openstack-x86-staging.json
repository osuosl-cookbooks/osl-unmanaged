{
    "chef_type": "environment",
    "cookbook_versions": {},
    "default_attributes": {
        "chef_client": {
            "config": {
                "ssl_verify_mode": ":verify_peer"
            }
        }
    },
    "description": "The OSL Openstack Staging Environment (x86)",
    "json_class": "Chef::Environment",
    "name": "osl-openstack-x86-staging",
    "override_attributes": {
        "apache": {
            "contact": "hostmaster@osuosl.org"
        },
        "developer_mode": true,
        "endpoints": {
            "bind-host": "140.211.167.174",
            "db": {
                "host": "10.1.0.208"
            },
            "host": "140.211.167.174",
            "mq": {
                "host": "10.1.0.208"
            }
        },
        "image": {
            "image_upload": true,
            "upload_image": {
                "cirros": "https://launchpad.net/cirros/trunk/0.3.0/+download/cirros-0.3.0-x86_64-disk.img"
            },
            "upload_images": [
                "cirros"
            ]
        },
        "mysql": {
            "allow_remote_root": true,
            "root_network_acl": "%"
        },
        "openstack": {
            "compute": {
                "config": {
                    "allow_same_net_traffic": "False",
                    "ram_allocation_ratio": 5.0
                },
                "libvirt": {
                    "bind_interface": "eth1",
                    "images_type": "lvm",
                    "virt_type": "kvm",
                    "volume_group": "nova"
                },
                "network": {
                    "force_dhcp_release": "true",
                    "libvirt_inject_key": "false",
                    "libvirt_inject_password": "false",
                    "multi_host": "true",
                    "public_interface": "eth0",
                    "service_type": "nova"
                },
                "networks": [
                    {
                        "bridge": "br-tenant",
                        "bridge_dev": "eth2",
                        "dns1": "140.211.166.130",
                        "dns2": "140.211.166.131",
                        "ipv4_cidr": "192.168.56.0/22",
                        "label": "private",
                        "multi_host": "T",
                        "network_size": "1024",
                        "num_networks": "1"
                    }
                ]
            },
            "dashboard": {
                "keystone_default_role": "_member_"
            }
        },
        "release": "havanah",
        "secret": {
            "key_path": "/etc/chef/encrypted_data_bag_secret"
        },
        "sysctl": {
            "net.ipv4.conf.all.rp_filter": "0",
            "net.ipv4.conf.default.rp_filter": "0",
            "net.ipv4.ip_forward": "1"
        },
        "yum": {
            "repo-key": "https://github.com/redhat-openstack/rdo-release/raw/master/RPM-GPG-KEY-RDO-Havana",
            "uri": "http://repos.fedorapeople.org/repos/openstack/openstack-havana/epel-6/"
        }
    },
    "yum": {
        "epel": {
            "baseurl": "http://epel.osuosl.org/6/$basearch",
            "enable": "true",
            "gpgkey": "http://epel.osuosl.org/RPM-GPG-KEY-EPEL-6"
        }
    }
}
