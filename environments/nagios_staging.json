{
  "name": "nagios_staging",
  "description": "For staging Nagios cookbook version 7",
  "cookbook_versions": {
    "nagios": "= 7.2.4",
    "nrpe": "= 1.5.2"
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
    "chef_client": {
      "init_style": "none",
      "cron": {
        "minute": "*/30",
        "hour": "*",
        "use_cron_d": true,
        "environment_variables": "/usr/bin/flock -n /tmp/chef-client.lock "
      }
  },
  "override_attributes": {
    "nagios": {
      "server": {
        "server_alias": "nagios3.osuosl.org"
      },
      "default_contact_groups": ["test"],
      "monitored_environments": [
        "openstack_production",
        "phpbb",
        "production",
        "phase_out_nginx"
      ]
    }
  }
}
