      "yum-epel": "= 0.3.0",
      "zlib": "= 2.0.0"
    },
    "default_attributes": {
        "chef_client": {
            "config": {
                "ssl_verify_mode": ":verify_peer"
            }
        }
    },
    "description": "The OSL Openstack Staging Environment (pp64)",
    "json_class": "Chef::Environment",
    "name": "osl-openstack-ppc64-staging",
    "override_attributes": {
        "apache": {
          "contact": "hostmaster@osuosl.org"
        },
        "openstack": {
            "compute": {
                "config": {
                    "allow_same_net_traffic": "False",
                    "ram_allocation_ratio": 5.0
                },
                "libvirt": {
                    "virt_type": "kvm"
                },
                "network": {
                    "force_dhcp_release": "true",
                    "multi_host": "true",
                    "public_interface": "enP2p1s0f2",
                    "service_type": "nova"
                },
                "networks": [
                    {
                        "bridge": "br-tenant",
                        "bridge_dev": "enP2p1s0f3",
                        "dns1": "140.211.166.130",
                        "dns2": "140.211.166.131",
                        "ipv4_cidr": "192.168.60.0/22",
                        "label": "private",
                        "multi_host": "T",
                        "network_size": "1024",
                        "num_networks": "1"
                    }
                ]
            },
            "dashboard": {
                "keystone_default_role": "_member_",
                "ssl": {
                  "cert": "horizon.pem",
                  "cert_url": "file:///etc/pki/tls/certs/wildcard.pem",
                  "chain": "wildcard-bundle.crt",
                  "key": "horizon.key",
                  "key_url": "file:///etc/pki/tls/private/wildcard.key"
                }
            },
            "db": {
                "block-storage": {
                    "db_name": "cinder_stg",
                    "username": "cinder_stg"
                },
                "compute": {
                    "db_name": "nova_stg",
                    "username": "nova_stg"
                },
                "dashboard": {
                    "db_name": "horizon_stg",
                    "username": "horizon_stg"
                },
                "identity": {
                    "db_name": "keystone_stg",
                    "username": "keystone_stg"
                },
                "image": {
                    "db_name": "glance_stg",
                    "username": "glance_stg"
                },
                "network": {
                    "db_name": "neutron_stg",
                    "username": "neutron_stg"
                },
                "orchestration": {
                    "db_name": "heat_stg",
                    "username": "heat_stg"
                },
                "telemetry": {
                    "db_name": "ceilometer_stg",
                    "username": "ceilometer_stg"
                }
            },
            "developer_mode": false,
            "endpoints": {
                "bind-host": "0.0.0.0",
                "db": {
                    "host": "140.211.15.221"
                },
                "host": "140.211.167.214",
                "mq": {
                    "host": "140.211.167.214"
                }
            },
            "image": {
                "image_upload": true,
                "upload_image": {
                    "debian-7": "http://ftp.osuosl.org/pub/osl/openpower/openstack/debian-7.qcow2",
                    "fedora-20": "http://ftp.osuosl.org/pub/osl/openpower/openstack/fedora-20.qcow2",
                    "ubuntu-14.04": "http://ftp.osuosl.org/pub/osl/openpower/openstack/ubuntu-14.04.qcow2",
                    "ubuntu-14.04-le": "http://ftp.osuosl.org/pub/osl/openpower/openstack/ubuntu-14.04-le.qcow2"
                },
                "upload_images": [
                    "debian-7",
                    "fedora-20",
                    "ubuntu-14.04",
                    "ubuntu-14.04-le"
                ]
            },
            "release": "icehouse",
            "secret": {
                "db_passwords_data_bag": "ppc64_staging_db_passwords",
                "key_path": "/etc/chef/encrypted_data_bag_secret",
                "secrets_data_bag": "ppc64_staging_secrets",
                "service_passwords_data_bag": "ppc64_staging_service_passwords",
                "user_passwords_data_bag": "ppc64_staging_user_passwords"
            },
            "sysctl": {
              "net.ipv4.conf.all.rp_filter": "0",
              "net.ipv4.conf.default.rp_filter": "0",
              "net.ipv4.ip_forward": "1"
            }
        },
        "yum": {
          "epel": {
            "baseurl": "http://epel.osuosl.org/6/$basearch",
            "enable": "true",
            "gpgkey": "http://epel.osuosl.org/RPM-GPG-KEY-EPEL-6"
          }
        }
    }
}
