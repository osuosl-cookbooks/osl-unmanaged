  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
    "chef_client": {
      "init_style": "none",
      "cron": {
        "minute": "*/30",
        "hour": "*",
        "use_cron_d": true,
        "environment_variables": "/usr/bin/flock -n /tmp/chef-client.lock ",
        "log_file": "/var/log/chef/client.log"
      },
      "config": {
        "ssl_verify_mode": ":verify_peer"
      }
    },
    "nagios": {
      "allowed_hosts": [
        "140.211.167.157",
        "10.1.0.100",
        "140.211.166.151",
        "10.1.0.66"
      ]
    },
    "nrpe": {
      "allowed_hosts": [
        "140.211.167.157",
        "10.1.0.100",
        "140.211.166.151",
        "10.1.0.66"
      ]
    }
  },
  "override_attributes": {
    "omnibus_updater": {
      "version": "12.4.3"
    }
  }
