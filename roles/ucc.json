{
  "run_list": [
    "role[base_managed]",
    "recipe[php]",
    "recipe[php::module_apc]",
    "recipe[osl-apache::project_ucc]",
    "recipe[osl-apache::munin]",
    "recipe[osl-apache::mysql]",
    "role[apache]",
    "role[project_projects]",
    "role[rdiff_backup]"
  ],
  "env_run_lists": {
  },
  "override_attributes": {
  },
  "description": "Role for ucc.osuosl.org",
  "chef_type": "role",
  "name": "ucc",
  "default_attributes": {
    "apache": {
        "prefork": {
            "serverlimit": "30",
            "maxclients": "30"
          }
    },
    "php": {
        "directives": {
            "post_max_size": "10M",
            "memory_limit": "256M",
            "upload_max_filesize": "10M"
        },
        "packages": [
            "php",
            "php-devel",
            "php-cli",
            "php-pear",
            "php-xml"
        ]
    }
  },
  "json_class": "Chef::Role"
}
