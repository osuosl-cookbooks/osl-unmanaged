{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "identity": {
        "ssl": {
          "cert_required": false
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {},
        "upload_images": []
      },
      "compute": {
        "conf": {
          "DEFAULT": {
            "disk_allocation_ratio": 5,
            "allow_resize_to_same_host": true
          }
        }
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "openstack": {
      "dashboard": {
        "server_hostname": "openpower-openstack.osuosl.org",
        "server_aliases": [ "openpower-controller.osuosl.org" ]
      }
    },
    "osl-openstack": {
      "database_suffix": "ppc64",
      "databag_prefix": "ppc64_mitaka",
      "endpoint_hostname": "openpower-controller.osuosl.org",
      "db_hostname": "mysql3.osuosl.bak",
      "nova_public_key": "ssh-rsa  nova@openstack-ppc64-mitaka",
      "image": {
        "glance_vol": "fs3.osuosl.bak:/openstack-glance"
      },
      "mon": {
        "check_nova_images": {
          "warning": 3,
          "critical": 5
        },
        "check_nova_hypervisors": {
          "warn_vcpus_percent": "0:200",
          "critical_vcpus_percent": "0:250"
        }
      },
      "vxlan_interface": {
        "controller": {
          "default": "eth1"
        },
        "compute": {
          "default": "enP3p9s0f3",
          "openpower3.osuosl.bak": "enP1p9s0f1",
          "openpower2.osuosl.bak": "enP3p5s0f1",
          "openpower5.osuosl.bak": "enP3p5s0f3",
          "openpower6.osuosl.bak": "enP3p5s0f3",
          "openpower8.osuosl.bak": "enP3p5s0f3"
        }
      },
      "physical_interface_mappings": [
        {
          "name": "public",
          "controller": {
            "default": "eth2"
          },
          "compute": {
            "default": "enP3p9s0f2",
            "openpower3.osuosl.bak": "enP1p9s0f0",
            "openpower2.osuosl.bak": "enP3p5s0f0",
            "openpower5.osuosl.bak": "enP3p5s0f2",
            "openpower6.osuosl.bak": "enP3p5s0f2",
            "openpower8.osuosl.bak": "enP3p11s0"
          }
        }
      ]
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack ppc64 Mitaka Role",
  "name": "openstack_ppc64_mitaka"
}
