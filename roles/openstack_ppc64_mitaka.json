{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "identity": {
        "ssl": {
          "cert_required": false
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {},
        "upload_images": []
      },
      "compute": {
        "conf": {
          "DEFAULT": {
            "disk_allocation_ratio": 3
          }
        }
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "firewall": {
      "iptables-config": {
        "save_on_restart": "yes",
        "save_on_stop": "yes"
      }
    },
    "openstack": {
      "dashboard": {
        "server_hostname": "openpower-openstack.osuosl.org",
        "server_aliases": [ "openpower-controller.osuosl.org" ]
      }
    },
    "osl-openstack": {
      "database_suffix": "ppc64",
      "databag_prefix": "ppc64_mitaka",
      "endpoint_hostname": "openpower-controller.osuosl.org",
      "db_hostname": "mysql3.osuosl.bak",
      "image": {
        "glance_vol": "fs3.osuosl.bak:/openstack-glance"
      },
      "mon": {
        "check_nova_images": {
          "warning": 3,
          "critical": 5
        }
      },
      "vxlan_interface": {
        "controller": {
          "default": "eth1"
        },
        "compute": {
          "default": "enP3p5s0f3",
          "openpower2.osuosl.bak": "enP3p5s0f1"
        }
      },
      "physical_interface_mappings": [
        {
          "name": "public",
          "controller": {
            "default": "eth2"
          },
          "compute": {
            "default": "enP3p5s0f2",
            "openpower2.osuosl.bak": "enP3p5s0f0"
          }
        }
      ]
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack ppc64 Mitaka Role",
  "name": "openstack_ppc64_mitaka"
}
