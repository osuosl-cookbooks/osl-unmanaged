{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-haproxy::osuosl_lb]",
    "recipe[osl-haproxy::ucarp]",
    "recipe[osl-haproxy::mon]",
    "recipe[ucarp::data_bag]"
  ],
  "chef_type": "role",
  "override_attributes": {
      "sysctl": {
        "params": {
          "net": {
            "netfilter.nf_conntrack_max": "256000",
            "nf_conntrack_max": "256000"
          }
        }
      },
      "osl-haproxy": {
        "cluster": "osuosl"
      },
      "ucarp": {
        "data_bag": {
          "cluster": "vip-lb1"
        }
      }
   },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "haproxy configuration for osuosl central load balancer",
  "name": "haproxy_osuosl"
}
