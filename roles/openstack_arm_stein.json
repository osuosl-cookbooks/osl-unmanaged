{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "ceph": {
      "admin": {
        "tag": "ceph-admin-x86"
      },
      "mon": {
        "role": "ceph_mon_x86",
        "tag": "ceph-mon-x86"
      },
      "osd": {
        "role": "ceph_osd_x86",
        "tag": "ceph-osd-x86"
      },
      "network": {
        "public": {
          "cidr": [
            "10.1.0.0/23",
            "10.1.2.0/23"
          ]
        },
        "cluster": {
          "cidr": [
            "10.1.0.0/23",
            "10.1.2.0/23"
          ]
        }
      }
    },
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "identity": {
        "ssl": {
          "cert_required": false
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {},
        "upload_images": []
      },
      "telemetry": {
        "pipeline": {
          "publishers": [
            "prometheus://mon2.osuosl.org:9091/metrics/job/ceilometer_arm"
          ]
        }
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "base": {
      "ifconfig": {
        "network_manager": false
      }
    },
    "openstack": {
      "dashboard": {
        "server_hostname": "arm-openstack.osuosl.org",
        "server_aliases": [ "arm-controller1.osuosl.org" ]
      }
    },
    "osl-openstack": {
      "cluster_role": "openstack_arm_stein",
      "ceph": {
        "image": true,
        "compute": true,
        "volume": true
      },
      "ceph_item": "openstack_arm",
      "cluster_name": "arm",
      "database_suffix": "arm",
      "databag_prefix": "arm",
      "endpoint_hostname": "arm-openstack.osuosl.org",
      "db_hostname": "vip-mysql2.osuosl.bak",
      "nova_public_key": "ssh-rsa  nova@openstack-x86-mitaka",
      "mon": {
        "check_cinder_services": {
          "warning": "0:",
          "critical": "0:"
        }
      },
      "compute": {
        "rbd_store_pool": "arm_vms"
      },
      "block": {
        "rbd_store_pool": "arm_volumes",
        "rbd_ssd_pool": "arm_volumes_ssd",
        "rbd_store_user": "arm_cinder"
      },
      "block_backup": {
        "rbd_store_pool": "arm_backups",
        "rbd_store_user": "arm_cinder-backup"
      },
      "image": {
        "rbd_store_pool": "arm_images",
        "rbd_store_user": "arm_glance"
      },
      "vxlan_interface": {
        "controller": {
          "default": "eth1"
        },
        "compute": {
          "default": "enp1s0"
        }
      },
      "external_networks": [
        "public4"
      ],
      "physical_interface_mappings": [
        {
          "name": "public4",
          "subnet": "140.211.167.64/27",
          "uuid": "48bfc43c-c99e-4395-afbd-97d02ef0116a",
          "controller": {
            "default": "eth3"
          },
          "compute": {
            "default": "enp1s0.107"
          }
        },
        {
          "name": "public5",
          "subnet": "140.211.169.0/26",
          "uuid": "2d0425cd-5898-434f-8669-f8ab2d5d0f72",
          "controller": {
            "default": "eth4"
          },
          "compute": {
            "default": "enp1s0.117"
          }
        },
        {
          "name": "public7",
          "subnet": "140.211.11.0/24",
          "controller": {
            "default": "eth5"
          },
          "compute": {
            "default": "enp1s0.105"
          }
        }
      ]
    },
    "osl-prometheus": {
      "openstack": {
        "cloud": "arm"
      }
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack arm Stein Role",
  "name": "openstack_arm_stein"
}
