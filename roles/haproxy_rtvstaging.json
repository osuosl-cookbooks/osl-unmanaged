{
  "env_run_lists": {},
  "run_list": [
    "recipe[certificate::manage_by_attributes]",
    "recipe[osl-haproxy]"
  ],
  "chef_type": "role",
  "override_attributes": {
      "haproxy": {
          "enable_ssl": "true",
          "member_port": "80",
          "ssl_member_port": "443",
          "ssl_incoming_port": "443 ssl crt /etc/pki/tls/certs/wildcard.pem ciphers RC4-SHA:HIGH:!aNULL:!MD5",
          "members": [
            {
              "hostname": "rtvstaging-web.osuosl.org",
              "ipaddress": "140.211.15.30"
            }
          ]
      },
      "certificate": [ {
        "wildcard": {
            "cert_file": "wildcard.pem",
            "key_file": "wildcard.key",
            "chain_file": "wildcard-bundle.crt",
            "combined_file": true
        }
      } ]
   },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "haproxy configuration for RTV Staging",
  "name": "haproxy_rtvstaging"
}
