{
  "env_run_lists": {},
  "run_list": [
    "recipe[certificate::manage_by_attributes]",
    "recipe[osl-haproxy]",
    "recipe[osl-haproxy::mon]"
  ],
  "chef_type": "role",
  "override_attributes": {
      "certificate": [ {
        "wildcard": {
            "cert_file": "wildcard.pem",
            "key_file": "wildcard.key",
            "chain_file": "wildcard-bundle.crt",
            "combined_file": true
        }
      } ],
      "osl-haproxy": {
          "enable_ssl_backend": true
      },
      "haproxy": {
          "enable_ssl": true,
          "member_port": 80,
          "ssl_member_port": 443,
          "ssl_incoming_port": "443 ssl crt /etc/pki/tls/wildcard.pem ciphers RC4-SHA:HIGH:!aNULL:!MD5",
          "ssl_httpchk": false,
          "members": [
            {
              "hostname": "rtvstaging-web1",
              "ipaddress": "140.211.15.30"
            },
            {
              "hostname": "rtvstaging-web2",
              "ipaddress": "140.211.15.117"
            }
          ]
      },
      "sysctl": {
        "params": {
          "net": {
            "netfilter.nf_conntrack_max": "256000",
            "nf_conntrack_max": "256000"
          }
        }
      },
      "users": ["osl-root", "osl-osuadmin", "rocky"]
   },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "haproxy configuration for RTV Staging",
  "name": "haproxy_rtvstaging"
}
