{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "ceph": {
      "admin": {
        "tag": "ceph-admin-x86"
      },
      "mon": {
        "role": "ceph_mon_x86",
        "tag": "ceph-mon-x86"
      },
      "osd": {
        "role": "ceph_osd_x86",
        "tag": "ceph-osd-x86"
      },
      "network": {
        "public": {
          "cidr": [
            "10.1.0.0/23",
            "10.1.2.0/23"
          ]
        },
        "cluster": {
          "cidr": [
            "10.1.0.0/23",
            "10.1.2.0/23"
          ]
        }
      }
    },
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "identity": {
        "ssl": {
          "cert_required": false
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {},
        "upload_images": []
      },
      "compute": {
        "conf": {
          "DEFAULT": {
            "cpu_allocation_ratio": "64.0",
            "ram_allocation_ratio": "1"
          }
        }
      },
      "secret": {
        "key_path": "/etc/cinc/encrypted_data_bag_secret"
      },
      "telemetry": {
        "pipeline": {
          "publishers": [
            "prometheus://mon2.osuosl.org:9091/metrics/job/ceilometer_x86"
          ]
        }
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "base": {
      "ifconfig": {
        "network_manager": false
      }
    },
    "openstack": {
      "dashboard": {
        "server_hostname": "openstack.osuosl.org",
        "server_aliases": [ "oprod-controller1.osuosl.org" ]
      }
    },
    "osl-openstack": {
      "cluster_role": "openstack_x86_stein",
      "ceph": {
        "image": true,
        "compute": true,
        "volume": true
      },
      "ceph_item": "openstack_x86",
      "cluster_name": "x86",
      "database_suffix": "x86",
      "databag_prefix": "x86",
      "endpoint_hostname": "oprod-controller1.osuosl.org",
      "db_hostname": "vip-mysql2.osuosl.bak",
      "nova_public_key": "ssh-rsa  nova@openstack-x86-mitaka",
      "image": {
        "glance_vol": "fs3.osuosl.bak:/openstack-glance"
      },
      "mon": {
        "check_cinder_services": {
          "warning": "0:",
          "critical": "0:"
        }
      },
      "vxlan_interface": {
        "controller": {
          "default": "eth1"
        },
        "compute": {
          "default": "ens802f0.42",
          "oprod1.osuosl.bak": "p1p1.42",
          "oprod2.osuosl.bak": "p1p1.42",
          "oprod3.osuosl.bak": "p1p1.42",
          "oprod4.osuosl.bak": "p1p1.42",
          "oprod15.osuosl.bak": "p6p1.42",
          "oprod16.osuosl.bak": "p6p1.42"
        }
      },
      "external_networks": [
        "general_servers1",
        "general_servers2",
        "public1",
        "public2",
        "backend"
      ],
      "physical_interface_mappings": [
        {
          "name": "general_servers1",
          "subnet": "140.211.166.0/25",
          "uuid": "e52f5435-0e93-4749-9317-ee350046a31d",
          "controller": {
            "default": "eth7"
          },
          "compute": {
            "default": "ens802f1.100",
