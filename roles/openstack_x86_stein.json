          "controller": {
            "default": "eth2"
          },
          "compute": {
            "default": "ens802f1.43",
            "oprod1.osuosl.bak": "p1p2.43",
            "oprod2.osuosl.bak": "p1p2.43",
            "oprod3.osuosl.bak": "p1p2.43",
            "oprod4.osuosl.bak": "p1p2.43",
