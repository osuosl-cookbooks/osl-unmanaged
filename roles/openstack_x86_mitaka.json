{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "identity": {
        "ssl": {
          "cert_required": false
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {},
        "upload_images": []
      },
      "compute": {
        "conf": {
          "DEFAULT": {
            "disk_allocation_ratio": 5
          }
        }
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "openstack": {
      "dashboard": {
        "server_hostname": "openstack.osuosl.org",
        "server_aliases": [ "oprod-controller1.osuosl.org" ]
      }
    },
    "osl-openstack": {
      "database_suffix": "x86",
      "databag_prefix": "x86_mitaka",
      "endpoint_hostname": "oprod-controller1.osuosl.org",
      "db_hostname": "mysql3.osuosl.bak",
      "nova_public_key": "ssh-rsa  nova@openstack-ppc64-mitaka",
      "image": {
        "glance_vol": "fs3.osuosl.bak:/openstack-glance"
      },
      "mon": {
        "check_nova_images": {
          "warning": 3,
          "critical": 5
       }
      },
      "vxlan_interface": {
        "controller": {
          "default": "eth1"
        },
        "compute": {
          "default": "enp4s0f0"
        }
      },
      "physical_interface_mappings": [
        {
          "name": "public",
          "controller": {
            "default": "eth2"
          },
          "compute": {
            "default": "enp4s0f1.108"
          }
        }
      ]
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack x86 Mitaka Role",
  "name": "openstack_x86_mitaka"
}
