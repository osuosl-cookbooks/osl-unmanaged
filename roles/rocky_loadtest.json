{
  "env_run_lists": {},
  "run_list": [
    "role[rocky_base]"
  ],
  "chef_type": "role",
  "override_attributes": {},
  "default_attributes": {
      "postfix": {
        "main": {
          "local_transport": "discard",
	  "relay_transport": "discard"
        }
      },
      "proj-rtv": {
        "data_bag": "rocky_loadtest",
        "environment": "loadtest",
        "passenger_poolsize": "120",
        "gluster": {
          "data": {
            "remotepath": "fs4.osuosl.bak:/rtvloadtest-data"
          }
        },
        "web": {
          "name": "rtv-loadtest.osuosl.org",
          "cert_file": "wildcard.pem",
          "cert_key": "wildcard.key",
          "cert_chain": "wildcard-bundle.crt"
        }
      },
      "certificate": [
        {
          "wildcard": {
            "cert_path": "/etc/pki/tls",
            "owner": "apache",
            "group": "apache",
            "cert_file": "wildcard.pem",
            "key_file": "wildcard.key",
            "chain_file": "wildcard-bundle.crt"
          }
        }
      ]
   },
  "json_class": "Chef::Role",
  "description": "Rocky load testing cluster",
  "name": "rocky_loadtest"
}
