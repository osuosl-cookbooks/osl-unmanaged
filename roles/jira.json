{
  "env_run_lists": {},
  "run_list": [
    "recipe[java]",
    "recipe[jira]",
    "recipe[base::jira]"
  ],
  "chef_type": "role",
  "override_attributes": {
  },
  "default_attributes": {
    "jira": {
      "install_type": "standalone",
      "apache2": {
        "ssl": {
          "certificate_file": "/etc/jira/certs/_.osuosl.org.pem",
          "key_file": "/etc/jira/certs/_.osuosl.org.key",
          "chain_file": "/etc/jira/certs/gd-bundle.crt"
        }
      }
    },
    "java": {
      "install_flavor": "oracle",
      "oracle": {
        "accept_oracle_download_terms": true
      }
    }
  },
  "json_class": "Chef::Role",
  "description": "jira server role",
  "name": "jira"
}
