{
  "env_run_lists": {},
  "run_list": [
    "role[base_managed]",
    "recipe[orvsd_central]",
    "recipe[orvsd_central::gluster]",
    "recipe[monitoring::nginx]",
    "recipe[monitoring::http]"
  ],
  "chef_type": "role",
  "override_attributes": {
      "orvsd_central":{
          "git-branch": "master",
          "gluster": {
              "moodle2-masters": {
                  "mountpath": "/data/moodle2-masters",
                  "remotepath": "fs1.orvsd.bak:/moodle2-masters"
              }
          },
          "vhost": "central.orvsd.org"
      }
  },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "orvsd py web server role",
  "name": "orvsd_py"
}
