{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "openstack": {
      "compute": {
        "networks": [
          {
            "bridge": "br-tenant",
            "bridge_dev": "eth1",
            "dns1": "140.211.166.130",
            "dns2": "140.211.166.131",
            "ipv4_cidr": "192.168.56.0/22",
            "label": "private",
            "multi_host": "T",
            "network_size": "1024",
            "num_networks": "1"
          }
        ]
      },
      "image": {
        "image_upload": true,
        "upload_image": {
          "cirros": "https://launchpad.net/cirros/trunk/0.3.0/+download/cirros-0.3.0-x86_64-disk.img"
        },
        "upload_images": [
          "cirros"
        ]
      }
    }
  },
  "default_attributes": {
    "osl-openstack": {
      "database_suffix": "xstg",
      "databag_prefix": "x86_staging",
      "vnc_bind_interface": {
        "controller": "eth1",
        "compute": "br42"
      }
    },
    "openstack": {
      "compute": {
        "network": {
          "libvirt_inject_key": "false",
          "libvirt_inject_password": "false",
          "public_interface": "br113"
        }
      },
      "dashboard": {
        "server_hostname": "openstack-staging.osuosl.org"
      },
      "endpoints": {
        "bind-host": "0.0.0.0",
        "db": {
          "host": "140.211.15.221"
        },
        "host": "140.211.167.174",
        "mq": {
          "host": "10.1.0.208"
        }
      }
    }
   },
  "json_class": "Chef::Role",
  "description": "Openstack x86 Staging Role",
  "name": "openstack_x86_staging"
}
