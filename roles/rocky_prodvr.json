{
  "env_run_lists": {},
  "run_list": [
    "role[rocky_base]"
  ],
  "chef_type": "role",
  "override_attributes": {},
  "default_attributes": {
      "proj-rtv": {
        "data_bag": "rocky_prodvr",
        "environment": "production",
        "gluster": {
          "data": {
            "remotepath": "fs4.osuosl.bak:/rtvprodvr-data"
          }
        },
        "web": {
          "name": "vr.rockthevote.com",
          "cert_file": "vr.rockthevote.com.pem",
          "cert_key": "vr.rockthevote.com.key",
          "cert_chain": "vr.rockthevote.com-bundle.crt"
        }
      },
      "certificate": [
        {
          "rtv-vr": {
            "cert_path": "/etc/pki/tls",
            "owner": "apache",
            "group": "apache",
            "cert_file": "vr.rockthevote.com.pem",
            "key_file": "vr.rockthevote.com.key",
            "chain_file": "vr.rockthevote.com-bundle.crt"
          }
        }
      ]
   },
  "json_class": "Chef::Role",
  "description": "Rocky Production vr cluster",
  "name": "rocky_prodvr"
}
