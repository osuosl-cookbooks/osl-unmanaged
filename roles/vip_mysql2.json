{
  "env_run_lists": {},
  "run_list": [
    "recipe[ucarp::data_bag]",
    "recipe[ucarp::remove]",
    "recipe[osl-keepalived::mysql2]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "osl-mysql": {
      "replication": {
        "role": "vip_mysql2"
      }
    },
    "percona": {
      "version": "5.6",
      "encrypted_data_bag": "vip_mysql2",
      "server": {
        "innodb_log_file_size": "64M"
      }
    },
    "ucarp": {
      "data_bag": {
        "cluster": "vip-mysql2"
      }
    }
  },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "vip_mysql2 server role",
  "name": "vip_mysql2"
}
