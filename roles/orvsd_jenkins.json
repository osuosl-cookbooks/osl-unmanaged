{
  "env_run_lists": {},
  "run_list": [
    "role[base_managed]",
    "recipe[orvsd-web::jenkins]",
    "role[apache]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "jenkins": {
      "http_proxy": {
        "ssl": {
          "redirect_http": true,
          "dir": "/etc/pki/tls/certs",
          "enabled": true,
          "key_path": "/etc/pki/tls/private/orvsd.key",
          "cert_path": "/etc/pki/tls/certs/_.orvsd.org.crt"
        }
      }
    }
  },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "orvsd jenkins role",
  "name": "orvsd_jenkins"
}
