{
  "env_run_lists": {},
  "run_list": [
    "recipe[proj-phpbb::haproxy]",
    "recipe[osl-haproxy::mon]",
    "recipe[osl-keepalived::haproxy_phpbb]"
  ],
  "chef_type": "role",
  "override_attributes": {
      "sysctl": {
        "params": {
          "net": {
            "netfilter.nf_conntrack_max": "256000",
            "nf_conntrack_max": "256000"
          }
        }
      },
      "ucarp": {
        "data_bag": {
          "cluster": "vip-phpbb-lb1"
        }
      }
   },
  "default_attributes": {},
  "json_class": "Chef::Role",
  "description": "haproxy configuration for phpbb",
  "name": "haproxy_phpbb"
}
