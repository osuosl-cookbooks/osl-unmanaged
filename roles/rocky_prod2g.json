{
  "env_run_lists": {},
  "run_list": [
    "role[rocky_base]"
  ],
  "chef_type": "role",
  "override_attributes": {},
  "default_attributes": {
      "proj-rtv": {
        "data_bag": "rocky_prod2g",
        "environment": "production",
        "gluster": {
          "data": {
            "remotepath": "fs4.osuosl.bak:/rtvprod2g-data"
          }
        },
        "web": {
          "name": "register2g.rockthevote.com",
          "cert_file": "register2g.rockthevote.com.pem",
          "cert_key": "register2g.rockthevote.com.key",
          "cert_chain": "register2g.rockthevote.com-bundle.crt"
        }
      },
      "certificate": [
        {
          "register2g-rtv": {
            "cert_path": "/etc/pki/tls",
            "owner": "apache",
            "group": "apache",
            "cert_file": "register2g.rockthevote.com.pem",
            "key_file": "register2g.rockthevote.com.key",
            "chain_file": "register2g.rockthevote.com-bundle.crt"
          }
        }
      ]
   },
  "json_class": "Chef::Role",
  "description": "Rocky Production 2g cluster",
  "name": "rocky_prod2g"
}
