{
  "env_run_lists": {},
  "run_list": [
    "recipe[rsyslog::default]",
    "role[base]",
    "recipe[nrpe]",
    "recipe[base::ifconfig]",
    "recipe[base::issue]",
    "recipe[base::managed]",
    "recipe[base::security]",
    "recipe[base::selinux]",
    "recipe[base::users]",
    "recipe[firewall::nrpe]",
    "recipe[firewall]",
    "recipe[monitoring]",
    "recipe[ntp]",
    "recipe[omnibus_updater]",
    "recipe[postfix::default]",
    "recipe[sudo]"
  ],
  "chef_type": "role",
  "override_attributes": {},
  "default_attributes": {
    "authorization": {
      "sudo": {
        "users": ["osuadmin"],
        "passwordless": true,
        "sudoers_defaults": [
         "!visiblepw",
          "env_reset",
          "env_keep =  \"COLORS DISPLAY HOSTNAME HISTSIZE INPUTRC KDEDIR LS_COLORS\"",
          "env_keep += \"MAIL PS1 PS2 QTDIR USERNAME LANG LC_ADDRESS LC_CTYPE\"",
          "env_keep += \"LC_COLLATE LC_IDENTIFICATION LC_MEASUREMENT LC_MESSAGES\"",
          "env_keep += \"LC_MONETARY LC_NAME LC_NUMERIC LC_PAPER LC_TELEPHONE\"",
          "env_keep += \"LC_TIME LC_ALL LANGUAGE LINGUAS _XKB_CHARSET XAUTHORITY\"",
          "env_keep += \"HOME\"",
          "always_set_home",
          "secure_path = /sbin:/bin:/usr/sbin:/usr/bin"
        ]
      }
    },
    "sysctl": {
      "params": {
        "kernel": {
          "msqmnb": 65536,
          "msqmax": 65536,
          "shmmax": 68719476736,
          "shmall": 4294967296
        },
        "net": {
          "core": {
            "rmem_max": "16777216",
            "wmem_max": "16777216"
          },
          "ipv4": {
            "tcp_rmem": "4096 87380 16777216",
            "tcp_wmem": "4096 87380 16777216",
            "tcp_no_metrics_save": 1,
            "netfiler": {
              "ip_conntrack_tcp_timeout_established": 14400
            }
          }
        }
      }
    },
    "postfix": {
      "mailtype": "client",
      "mydomain": "osuosl.org",
      "myorigin": "osuosl.org",
      "relayhost": "osuosl.org"
    },
    "openssh": {
      "server": {
        "password_authentication": "no"
      }
    },
    "ntp": {
      "servers": ["time.oregonstate.edu", "pool.ntp.org"]
    },
    "mysql": {
      "version": "5.5"
    },
    "ldap": {
      "uri": "ldaps://ldap1.osuosl.org ldaps://ldap2.osuosl.org",
      "base": "dc=osuosl,dc=org"
    },
    "rsyslog": {
      "server_search": "role:logstash_server",
      "port": 5000,
      "preserve_fqdn": "on"
    }
  },
  "json_class": "Chef::Role",
  "description": "base managed role for all managed systems",
  "name": "base_managed-dev2"
}
