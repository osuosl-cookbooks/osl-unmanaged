{
  "env_run_lists": {},
  "run_list": [
    "recipe[proj-rtv::web]",
  ],
  "chef_type": "role",
  "override_attributes": {},
  "default_attributes": {
      "apache": {
        "mpm": "worker",
        "binary": "/usr/sbin/httpd.worker",
        "keepalive": "Off",
        "keepaliverequests": 0,
        "keepalivetimeout": 2,
        "worker": {
          "startservers": 4,
          "serverlimit": 17,
          "maxclients": 2550,
          "minsparethreads": 64,
          "maxsparethreads": 2550,
          "threadsperchild": 150,
          "maxrequestsperchild": 10000
        },
      },
   },
  "json_class": "Chef::Role",
  "description": "Rocky web server",
  "name": "rocky_web"
}
