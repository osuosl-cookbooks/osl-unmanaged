{
  "env_run_lists": {
  },
  "chef_type": "role",
  "run_list": [
    "role[base_managed]",
    "recipe[php]",
    "recipe[osl-apache]",
    "recipe[osl-apache::project_apereo]",
    "recipe[osl-apache::mysql]",
    "role[apache]",
    "role[project_projects]",
    "role[rdiff_backup]",
    "recipe[php::module_gd]",
    "recipe[php::module_apc]",
    "recipe[git]",
    "recipe[mysql::client]",
    "recipe[osl-sudo::apereo]",
    "recipe[osl-drupal::drush]",
    "recipe[certificate::manage_by_attributes]"
  ],
  "override_attributes": {
    "php": {
      "directives": {
        "post_max_size": "10M",
        "memory_limit": "512M",
        "upload_max_filesize": "10M"
      }
    }
  },
  "json_class": "Chef::Role",
  "description": "apereo.osuosl.org role",
  "name": "apereo",
  "default_attributes": {
    "certificate": [
      {
        "wwwapereoorg": {
          "cert_path": "/etc/pki/tls",
          "owner": "apache",
          "group": "apache",
          "cert_file": "www.apereo.org.pem",
          "key_file": "www.apereo.org.key",
          "chain_file": "www.apereo.org-bundle.crt"
        }
      },
      {
        "wwwsakaiprojectorg": {
          "cert_path": "/etc/pki/tls",
          "owner": "apache",
          "group": "apache",
          "cert_file": "www.sakaiproject.org.pem",
          "key_file": "www.sakaiproject.org.key",
          "chain_file": "www.sakaiproject.org-bundle.crt"
        }
      }
    ]
  }
}
