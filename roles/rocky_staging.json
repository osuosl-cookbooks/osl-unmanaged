{
  "env_run_lists": {},
  "run_list": [
    "role[rocky_base]"
  ],
  "chef_type": "role",
  "override_attributes": {},
  "default_attributes": {
      "proj-rtv": {
        "data_bag": "rocky_staging",
        "environment": "staging",
        "monitor_mode": "false",
        "gluster": {
          "data": {
            "remotepath": "fs3.osuosl.bak:/rtvstaging-data"
          }
        },
        "web": {
          "name": "rtvstaging.osuosl.org",
          "server_aliases": [ "rtvstaging-lb.osuosl.org" ],
          "cert_file": "wildcard.pem",
          "cert_key": "wildcard.key",
          "cert_chain": "wildcard-bundle.crt"
        }
      },
      "certificate": [
        {
          "wildcard": {
            "cert_path": "/etc/pki/tls",
            "owner": "apache",
            "group": "apache",
            "cert_file": "wildcard.pem",
            "key_file": "wildcard.key",
            "chain_file": "wildcard-bundle.crt"
          }
        }
      ]
   },
  "json_class": "Chef::Role",
  "description": "Rocky Staging cluster",
  "name": "rocky_staging"
}
