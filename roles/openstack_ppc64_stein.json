{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-openstack]"
  ],
  "chef_type": "role",
  "override_attributes": {
    "ceph": {
      "admin": {
        "tag": "ceph-admin-ppc64"
      },
      "mon": {
        "role": "ceph_mon_ppc64",
        "tag": "ceph-mon-ppc64"
      },
      "osd": {
        "role": "ceph_osd_ppc64",
        "tag": "ceph-osd-ppc64"
      },
      "network": {
        "public": {
          "cidr": [
            "10.1.0.0/23"
          ]
        },
        "cluster": {
          "cidr": [
            "10.1.0.0/23"
          ]
        }
      }
    },
    "ibm-power": {
      "ibm-power-tools": {
        "skip_if_unavailable": true,
        "retries": 1,
        "timeout": "10"
      }
    },
    "openstack": {
      "mq": {
        "user": "admin"
      },
      "identity": {
        "ssl": {
          "cert_required": false
        }
      },
      "image": {
        "image_upload": true,
        "upload_image": {},
        "upload_images": []
      },
      "compute": {
        "conf": {
          "DEFAULT": {
            "disk_allocation_ratio": 5,
            "allow_resize_to_same_host": true
          }
        }
      },
      "telemetry": {
        "pipeline": {
          "publishers": [
            "prometheus://mon3.osuosl.org:9091/metrics/job/ceilometer_ppc64"
          ]
        }
      }
    }
  },
  "default_attributes": {
    "authorization": {
      "sudo": {
        "include_sudoers_d": true
      }
    },
    "base": {
      "ifconfig": {
        "network_manager": false
      }
    },
    "openstack": {
      "dashboard": {
        "server_hostname": "openpower-openstack.osuosl.org",
        "server_aliases": [
          "openpower-controller.osuosl.org"
        ]
      }
    },
    "osl-openstack": {
      "cluster_role": "openstack_ppc64_stein",
      "ceph": {
        "image": true,
        "compute": true,
        "volume": true
      },
      "ceph_item": "openstack_ppc64",
      "cluster_name": "ppc64",
      "database_suffix": "ppc64",
      "databag_prefix": "ppc64",
      "endpoint_hostname": "openpower-controller.osuosl.org",
      "db_hostname": "vip-mysql2.osuosl.bak",
      "nova_public_key": "ssh-rsa  nova@openstack-ppc64-mitaka",
      "image": {
        "glance_vol": "fs3.osuosl.bak:/openstack-glance"
      },
      "vxlan_interface": {
        "controller": {
          "default": "eth1"
        },
        "compute": {
          "default": "enP4p1s0.42",
          "openpower4.osuosl.bak": "enP48p1s0f0.42",
          "openpower7.osuosl.bak": "enP48p1s0f0.42",
          "openpower9.osuosl.bak": "enP48p1s0f0.42",
          "openpower10.osuosl.bak": "enP48p1s0f0.42",
          "openpower11.osuosl.bak": "enp1s0f2.42",
          "openpower12.osuosl.bak": "enP48p1s0f0.42",
          "oph1.osuosl.bak": "enp1s0f0",
          "oph2.osuosl.bak": "enp1s0f0"
        }
      },
      "external_networks": [
        "public",
        "public3"
      ],
      "physical_interface_mappings": [
        {
          "name": "public",
          "subnet": "140.211.168.0/24",
          "uuid": "a5bd5b53-1dc4-423c-a854-aa1cc9ee5456",
          "controller": {
            "default": "eth2"
          },
          "compute": {
            "default": "enP4p1s0.108",
            "openpower4.osuosl.bak": "enP48p1s0f0.108",
            "openpower7.osuosl.bak": "enP48p1s0f0.108",
            "openpower9.osuosl.bak": "enP48p1s0f0.108",
            "openpower10.osuosl.bak": "enP48p1s0f0.108",
            "openpower11.osuosl.bak": "enp1s0f2.108",
            "openpower12.osuosl.bak": "enP48p1s0f0.108",
            "oph1.osuosl.bak": "enp1s0f1.108",
            "oph2.osuosl.bak": "enp1s0f1.108"
          }
        },
        {
          "name": "public3",
          "subnet": "140.211.169.128/26",
          "uuid": "02b4376c-179c-412d-94c9-e56fdb2a7caf",
          "controller": {
            "default": "eth3"
          },
          "compute": {
            "default": "enP4p1s0.112",
            "openpower4.osuosl.bak": "enP48p1s0f0.112",
            "openpower7.osuosl.bak": "enP48p1s0f0.112",
            "openpower9.osuosl.bak": "enP48p1s0f0.112",
            "openpower10.osuosl.bak": "enP48p1s0f0.112",
            "openpower11.osuosl.bak": "enp1s0f2.112",
            "openpower12.osuosl.bak": "enP48p1s0f0.112",
            "oph1.osuosl.bak": "enp1s0f1.112",
            "oph2.osuosl.bak": "enp1s0f1.112"
          }
        },
        {
          "name": "public6",
          "subnet": "140.211.10.65/26",
          "uuid": "658bfca4-d3d5-4441-8389-6bab8e3b2d30",
          "controller": {
            "default": "eth4"
          },
          "compute": {
            "default": "enP4p1s0.116",
            "openpower4.osuosl.bak": "enP48p1s0f0.116",
            "openpower7.osuosl.bak": "enP48p1s0f0.116",
            "openpower9.osuosl.bak": "enP48p1s0f0.116",
            "openpower10.osuosl.bak": "enP48p1s0f0.116",
            "openpower11.osuosl.bak": "enp1s0f2.116",
            "openpower12.osuosl.bak": "enP48p1s0f0.116",
            "oph1.osuosl.bak": "enp1s0f1.116",
            "oph2.osuosl.bak": "enp1s0f1.116"
          }
        }
      ]
    },
    "osl-prometheus": {
      "openstack": {
        "cloud": "ppc64"
      }
    }
  },
  "json_class": "Chef::Role",
  "description": "Openstack ppc64 Stein Role",
  "name": "openstack_ppc64_stein"
}
