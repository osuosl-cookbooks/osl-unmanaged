{
  "env_run_lists": {},
  "run_list": [
    "recipe[osl-apache::default]",
    "recipe[apache2::mod_ssl]",
    "role[apache]",
    "recipe[ganeti_webmgr::default]",
    "recipe[firewall::gwm]",
    "recipe[certificate::wildcard]"
  ],
  "chef_type": "role",
  "override_attributes": {},
  "default_attributes": {
    "ganeti_webmgr": {
      "migrate": true,
      "apache": {
        "server_name": "ganeti.osuosl.org",
        "server_aliases": [
          "ganeti.osuosl.org"
        ]
      },
      "https_enabled": true,
      "certificate_key_file": "/etc/pki/tls/private/wildcard.key",
      "certificate_file": "/etc/pki/tls/certs/wildcard.pem",
      "certificate_chain_file": "/etc/pki/tls/certs/wildcard-bundle.crt",
      "revision": "master",
      "debug": false,
      "databag_item": "production",
      "database": {
        "user": "ganeti_webmgr",
        "port": "3306",
        "host": "vip-mysql2.osuosl.org",
        "engine": "django.db.backends.mysql",
        "name": "ganeti_webmgr"
        }
      }
    },
  "json_class": "Chef::Role",
  "description": "Ganeti Web Manager server role",
  "name": "gwm"
}
